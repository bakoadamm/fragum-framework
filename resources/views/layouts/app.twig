    <style>

        #overlay {
            height: 100vh;
            width: 100vw;
            background: rgba(255, 255, 255, 1);
            position: fixed;
            left: 0;
            top: 0;
            z-index: 3000;
        }

        .spinner{
            width: 80px;
            height: 80px;

            border: 2px solid #f3f3f3;
            border-top:3px solid #03895d;
            border-radius: 100%;

            position: absolute;
            top:0;
            bottom:0;
            left:0;
            right: 0;
            margin: auto;

            animation: spin 1s infinite linear;
        }

        @keyframes spin {
            from{
                transform: rotate(0deg);
            }to{
                 transform: rotate(360deg);
             }
        }

    </style>
    {% block css %}{% endblock %}

